function ids_openIncompatibleLightbox(btn, target) {
    var box = $(target), wrap, content;
    if (box.children('.lightBoxWrap').length) { wrap = box.children('.lightBoxWrap') }
    else if (box.children('.sp_lightBoxWrap').length) { wrap = box.children('.sp_lightBoxWrap') }
    content = wrap.children('.contentBody');
    if ($(btn).hasClass('disabled')) { return false }
    content.css({ 'height': '', 'overflow-y': '' });
    wrap.css({ 'margin-top': '', 'margin-left': '' });
    box.removeClass('hide');
    if (wrap.height() > $(window).height()) { content.css({ 'height': $(window).height() - (wrap.height() - content.height()) - 60 }) }
else { content.css({ 'height': '', 'overflow-y': '' }) }
wrap.css({ 'margin-top': 0 - (wrap.height() / 2), 'margin-left': 0 - (wrap.width() / 2) });
}

//Custom Select Box Virgin Mobile
function DCXSelectBoxes() {
    $('div.selectBox').each(function () {
        $(this).children('.selected1,.selected2').html($(this).children('.selectOptions,.selectOptions2').children('.selectOption:first').html());
        $(this).attr('value', $(this).children('.selectOptions,.selectOptions2').children('.selectOption:first').attr('value'));

        $(this).children('.selected1,.selectArrow,.selected2,.selectArrow2').click(function () {
            if ($(this).parent().children('.selectOptions,.selectOptions2').css('display') == 'none') {
                $(this).parent().children('.selectOptions,.selectOptions2').css('display', 'block');
            }
            else {
                $(this).parent().children('.selectOptions,.selectOptions2').css('display', 'none');
            }
        });

        $(this).find('.selectOption').click(function () {
            $(this).parent().css('display', 'none');
            $(this).closest('.selectBox').attr('value', $(this).attr('value'));
            $(this).parent().siblings('.selected1,.selected2').html($(this).html());
        });
    });
}
//Bell Shop Side Menu Global Connector
function shopSideMenu() {
    $('.sideMenu').mouseover(function () {
        var $sideMenu = $('#' + $(this).attr('id') + 'Content');
        $('.sideMenuContent').addClass('hide');
        $sideMenu.removeClass('hide');
        rightSideSameHeight()
    });
    $(".button22").mouseover(function () {
        $(".sideMenuContent").addClass('hide');
    });
}
//Make the right side of the shop menu to the same height as left side if it is shorter than the left side
//var debugTop = 0
function rightSideSameHeight() {
    var leftSideHeight = $('#menuShop').height(), menuShadowH = 28 //this is to adjust because the flyout does not have shadow;
    leftSideHeight -= menuShadowH
    //$('body').append('<div style="position:fixed; top:' + debugTop + 'px;">' + leftSideHeight + '</div>')
    //debugTop += 20
    $('.sideMenuRight').each(function () {
        var flyout = $(this);
        flyout.find('.globalConnectorFlyout').css('height', '');
        var rightSideHeight = flyout.height();
        //$('body').append('<div style="position:fixed; top:' + debugTop + 'px;">' + rightSideHeight + '</div>')
        //debugTop += 20
        if (rightSideHeight == 0) { }
        else {
            //$('body').append('<div style="position:fixed; top:' + debugTop + 'px;">' + rightSideHeight + '</div>')
            //debugTop += 20
            if (rightSideHeight - 33 < leftSideHeight) {
                flyout.find('.globalConnectorFlyout').css('height', leftSideHeight);
            }
        }
    });
    $('.sideMenuContent').each(function () {
        var hover = $(this).find('.sideMenuHover');
        hover.css('height', leftSideHeight + 10);
    });
    if ($(".shopServicesBtn").hasClass("hide")) {
        $('.menuBridge1,.menuItem1').css({ 'top': 12 });
        $('.menuBridge2,.menuItem2').css({ 'top': 103 });
        $('.menuBridge3,.menuItem3').css({ 'top': 194 });
        $('.menuBridge4,.menuItem4').css({ 'top': 285 });
        $('.menuBridge5,.menuItem5').css({ 'top': 376 });
    }
}
//Tabs content
$(document).ready(function () {
    $(".tabs-menu a").click(function (event) {
        event.preventDefault();
        $(this).parent().addClass("active");
        $(this).parent().siblings().removeClass("active");
        var tab = $(this).attr("href");
        $(".tab-content").not(tab).css("display", "none");
        $(tab).fadeIn();
    });
});
