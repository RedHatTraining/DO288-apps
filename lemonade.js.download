$(document).ready(function () {
    initEventDelegateListeners();

    $.fn.extend({
        equalHeights: function () {
            var top = 0;
            var row = [];
            var classname = ('equalHeights' + Math.random()).replace('.', '');
            $(this).each(function () {
                var thistop = $(this).offset().top;
                if (thistop > top) {
                    $('.' + classname).removeClass(classname);
                    top = thistop;
                }
                $(this).addClass(classname);
                $(this).height('auto');
                var h = (Math.max.apply(null, $('.' + classname).map(function () { return $(this).height(); }).get()));
                $('.' + classname).height(h);
            }).removeClass(classname);
        }
    });

    $(function () {
        $(window).resize(function () {
            $('.eqHeight').equalHeights();
            if ($('sameHeightCo')) {
                $('.sameHeightCo').equalHeights();
            }



        }).trigger('resize');
    });

    //var $li = $(".colGroup.bord4 ul li");
    //var maxHeight = $li.map(function () {
    //    return $(this).height();
    //}).get();
    //$li.height(Math.max.apply(this, maxHeight));
});

function initEventDelegateListeners() {
    var $body = $('body');

    if (!$body.hasClass('init-event-delegate')) {
        $body.addClass('init-event-delegate');

        $('body').on('keydown', '.click-on-space-or-enter-delegate', function (e) {
            var keyCode = e.which || e.keyCode || 0;

            if (32 === keyCode || 13 === keyCode) {
                e.preventDefault();
                e.stopPropagation();
                this.click();
            }
        });

        $('body').on('keydown', '.click-on-space-delegate', function (e) {
            if (32 === (e.which || e.keyCode || 0)) {
                e.preventDefault();
                e.stopPropagation();
                this.click();
            }
        });
    }
}

$(document).ready(function () {
    var i = 0, j = 0;
    $('.colGroup.bord4 ul li').each(function () {
        j = $(this).outerHeight();
        if (i < j)
            i = j;
    });
    $('.colGroup.bord4 ul li').each(function () {
        $(this).height(i);
       
    });
});


$(document).ready(function () {
    $('input[type="checkbox"].cartInp').click(function () {
        $(this).parents().toggleClass("activatedDivs");
        $('.col-eq').colequalizer();
    });
    $('header').addClass('MYLHeaderFocusOutline');
});

function fn_LemonLighBoxCloseOverride(divId) {
    var objDiv = document.getElementById(divId);
    if (objDiv != null) {
        //DIV > div(lblTitle) > div(padder) & <a>
        var varCloseBtn = $(objDiv).find('.lbTitle').find('.lightBoxCloseJs');
        if (varCloseBtn.html() != null)
            varCloseBtn.html('<i class="icon-close-x" aria-hidden="true" style="font-weight:bolder;"></i>');
    }
}